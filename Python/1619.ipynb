   "source": [
    "from numpy import mean \n",
    "\n",
    "class Solution(object):\n",
    "    def trimMean(self,arr):\n",
    "        arr.sort()\n",
    "        n = int(0.05*len(arr))\n",
    "        avg = mean(arr[n:len(arr)-n])\n",
    "        return avg\n",
    "        \n",
    "\n",
    "\n",
    "sol = Solution()\n",
    "arr = [1,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,2,3]\n",
    "print(sol.trimMean(arr))\n"
   ]
  }
